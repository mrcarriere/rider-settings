<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Source Code Pro" />
    <option name="LINE_SPACING" value="0.9" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Menlo" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\bNotImplementedException\b" />
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;OPT)(\W|$)(.*)">
      <option name="FOREGROUND" value="c0a527" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TEMP)(\W|$)(.*)" />
    <pattern icon="question" useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)">
      <option name="FOREGROUND" value="6cc02e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;FIXME)(\W|$)(.*)">
      <option name="FOREGROUND" value="c0423e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="c0423e" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)">
      <option name="FOREGROUND" value="c0423e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="c0423e" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;NOTE)(\W|$)(.*)">
      <option name="FOREGROUND" value="c0a527" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <filter name="Needs Work">
      <pattern index="4" />
      <pattern index="5" />
      <pattern index="0" />
      <pattern index="3" />
      <pattern index="1" />
    </filter>
  </component>
</application>